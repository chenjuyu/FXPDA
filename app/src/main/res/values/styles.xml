    <style name="slide">
        <item name="android:layout_width">7dip</item>
        <item name="android:layout_height">7dip</item>
        <item name="android:layout_marginLeft">5dip</item>
        <item name="android:layout_marginRight">5dip</item>
    </style>

    <style name="CustomWindowTitleBackground">
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="MGallery" parent="@android:style/Widget.Gallery">
        <item name="android:fadingEdge">none</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:spacing">1dip</item>
        <item name="android:unselectedAlpha">1</item>
    </style>

    <style name="mSrollView">
        <item name="android:fadingEdge">none</item>
        <item name="android:scrollbarThumbVertical">@drawable/scrollbar_handle_vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="customScrollView" parent="@android:style/Widget.ScrollView">
        <item name="android:scrollbarThumbVertical">@drawable/scrollbar_handle_vertical</item>
    </style>

    <style name="customListView" parent="@android:style/Widget.ListView">
        <item name="android:scrollbarThumbVertical">@drawable/scrollbar_handle_vertical</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:listSelector">@drawable/home_itme_selector</item>
    </style>

    <style name="Default" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowTitleSize">40.0dip</item>
        <item name="android:windowNoTitle">false</item>
        <item name="android:listViewStyle">@style/customListView</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowTitleBackgroundStyle">@style/CustomWindowTitleBackground</item>

        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="top_condition_div">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">@drawable/software_textcolor</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">#ffffff</item>
    </style>

    <style name="AnimTools" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_in</item>
        <item name="android:windowExitAnimation">@anim/push_out</item>
    </style>

 <!--  模块的设置-->

    <style name="SingleChoose">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="item_textColor">#666666</item>
        <item name="item_backgroundColor">#ffffff</item>
        <item name="item_selectedTextColor">#ffffff</item>
        <item name="item_selectedBackgroundColor">#7289ff</item>
        <item name="item_radius">0dp</item>
        <item name="item_strokeColor">#939393</item>
        <item name="item_selectedStrokeColor">#939393</item>
        <item name="item_strokeWidth">1px</item>
        <item name="item_horizontalSpacing">10dp</item>
        <!--这里设置固定大小,padding为0-->
        <item name="item_horizontalPadding">10dp</item>
        <item name="item_verticalPadding">5dp</item>
        <!--单位dp-->
        <item name="item_width">80</item>
        <item name="item_height">wrap_content</item>
        <!--限制tag显示长度,规则查看Textview-->
        <item name="item_maxEms">5</item>
        <!--单选-->
        <item name="item_multiChooseable">false</item>
        <item name="item_click">false</item>
    </style>

    <style name="FlowLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="item_textColor">#666666</item>
        <item name="item_backgroundColor">#ffffff</item>
        <item name="item_selectedTextColor">#ffffff</item>
        <item name="item_selectedBackgroundColor">#7289ff</item>
        <item name="item_radius">0dp</item>
        <item name="item_strokeColor">#939393</item>
        <item name="item_selectedStrokeColor">#939393</item>
        <item name="item_strokeWidth">1px</item>
        <item name="item_horizontalSpacing">10dp</item>
        <!--这里设置固定大小,padding为0-->
        <item name="item_horizontalPadding">10dp</item>
        <item name="item_verticalPadding">5dp</item>
        <!--单位dp-->
        <item name="item_width">wrap_content</item>
        <item name="item_height">wrap_content</item>
        <!--限制tag显示长度,规则查看Textview-->
        <item name="item_maxEms">5</item>
        <!--多选-->
        <item name="item_multiChooseable">false</item>
